{
  "name": "@carrotwu/wcli",
  "version": "1.0.4",
  "description": "A global cli tool that supports custom development, packaging and pushing",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node-dev --respawn --transpileOnly  src/main.ts",
    "lint": "eslint src --ext .ts --fix"
  },
  "bin": {
    "wcli": "./dist/main.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "ts",
    "cli",
    "node",
    "typescript"
  ],
  "author": "carrotWu",
  "license": "MIT",
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/fs-extra": "^9.0.6",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.14.21",
    "@types/update-notifier": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "colors": "^1.4.0",
    "commander": "^7.0.0",
    "compressing": "^1.5.1",
    "download-git-repo": "^3.0.2",
    "enquirer": "^2.3.6",
    "fs-extra": "^9.0.1",
    "module-alias": "^2.2.2",
    "ora": "^5.2.0",
    "simple-git": "^2.31.0",
    "update-notifier": "^5.0.1"
  },
  "_moduleAliases": {
    "@utils": "dist/utils",
    "@srcTypes": "dist/srcTypes",
    "@constants": "dist/constants",
    "@commands": "dist/commands"
  }
}
